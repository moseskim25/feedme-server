/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model feedback {
  id           BigInt   @id(map: "insights_pkey") @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  logical_date String
  content      String
  user_id      String   @db.Uuid
  user         user     @relation(fields: [user_id], references: [id], onDelete: Cascade, map: "summary_user_id_fkey")

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model food {
  id                 BigInt               @id @default(autoincrement())
  created_at         DateTime             @default(now()) @db.Timestamptz(6)
  image_prompt       String?
  description        String?
  r2_key             String?
  user_id            String               @db.Uuid
  logical_date       String
  deleted_at         DateTime?            @db.Timestamptz(6)
  user               user                 @relation(fields: [user_id], references: [id], onDelete: Cascade)
  food_group_serving food_group_serving[]

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model food_group {
  id                 BigInt               @id @default(autoincrement())
  created_at         DateTime             @default(now()) @db.Timestamptz(6)
  name               String               @unique
  description        String?
  food_group_serving food_group_serving[]

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model food_group_serving {
  id            BigInt     @id @default(autoincrement())
  created_at    DateTime   @default(now()) @db.Timestamptz(6)
  food_id       BigInt
  food_group_id BigInt
  servings      Float      @db.Real
  user_id       String?    @db.Uuid
  food_group    food_group @relation(fields: [food_group_id], references: [id], onDelete: Cascade)
  food          food       @relation(fields: [food_id], references: [id], onDelete: Cascade)
  user          user?      @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model message {
  id           BigInt   @id(map: "log_pkey") @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  user_id      String?  @db.Uuid
  content      String
  role         String
  logical_date String?
  is_processed Boolean? @default(false)

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model symptom {
  id           BigInt   @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  description  String
  user_id      String?  @db.Uuid
  logical_date String
  user         user?    @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@schema("public")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model user {
  created_at           DateTime             @default(now()) @db.Timestamptz(6)
  username             String?
  website              String?
  avatarUrl            String?
  id                   String               @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  isOnboardingComplete Boolean?
  name                 String?
  feedback             feedback[]
  food                 food[]
  food_group_serving   food_group_serving[]
  symptom              symptom[]
  user_job             user_job[]

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model user_job {
  id           BigInt    @id @default(autoincrement())
  created_at   DateTime  @default(now()) @db.Timestamptz(6)
  completed_at DateTime? @db.Timestamptz(6)
  description  String?
  user_id      String?   @db.Uuid
  user         user?     @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@schema("public")
}

